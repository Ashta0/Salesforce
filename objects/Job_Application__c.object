    <validationRules>
        <fullName>Formula_Rule_6</fullName>
        <active>true</active>
        <description>If average rating is less than 3, you cannot hire or extend an offer.</description>
        <errorConditionFormula>IF( (Average_Rating__c &lt; 3 ||  ISNULL(Average_Rating__c)) ,  IF((ISPICKVAL( Status__c , &quot;Extend an Offer&quot;) || ISPICKVAL( Status__c , &quot;Hired&quot;)),true,false) , false)</errorConditionFormula>
        <errorMessage>If average rating is less than 3, you cannot hire or extend an offer.</errorMessage>
    </validationRules>
